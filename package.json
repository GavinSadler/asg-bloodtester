{
  "name": "asg-bloodtester",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "remoteMachine": "10.248.52.178",
  "scripts": {
    "runFrontend": "vite --cors ./frontend",
    "buildFrontend": "tsc && vite build ./frontend --outDir ../dist",
    "previewFrontend": "vite preview",
    "deployFrontend": "ssh pi@%npm_package_remoteMachine% 'rm -rf ~/www' && scp -r ./dist pi@%npm_package_remoteMachine%:~/www && ssh pi@%npm_package_remoteMachine% './scripts/reload_fullpageos_txt'",
    "runBackend": "cd ./backend && python Server.py --debug",
    "pushBackend": "ssh pi@%npm_package_remoteMachine% 'rm -rf ~/backend' && rm -rf ./backend/__pycache__ && scp -r ./backend pi@%npm_package_remoteMachine%:~/backend",
    "deployBackend": "yarn pushBackend && ssh pi@%npm_package_remoteMachine% 'systemctl --user restart backend.service'",
    "deploy": "yarn buildFrontend && yarn deployFrontend && yarn deployBackend",
    "ssh": "ssh pi@%npm_package_remoteMachine% -L 5000:127.0.0.1:5000 -L 80:127.0.0.1:80"
  },
  "dependencies": {
    "plotly.js": "^2.29.0",
    "preact": "^10.19.3",
    "preact-compat": "^3.19.0",
    "react-plotly": "^1.0.0",
    "react-plotly.js": "^2.6.0",
    "react-range": "^1.8.14",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.7.0",
    "@types/plotly.js": "^2.29.0",
    "@types/react-plotly.js": "^2.6.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
